record(acalcout, "calibrated_cvr_lru")
{
	field(SCAN, "Passive")
	field(NELM, 20)
	field(INAA, "raw_cvr_lru")
	field(INPA,"attenuation_db")
        field(INPB,"probe_factor")
        field(CALC, "(AA*10**(A/20))/B")

}	

